{
  "name": "gatsby-starter-default",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "version": "0.1.0",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "dependencies": {
    "@testing-library/cypress": "^6.0.0",
    "@types/react": "^16.9.35",
    "axe-core": "^3.5.3",
    "bufferutil": "^4.0.1",
    "cypress": "^4.6.0",
    "cypress-axe": "^0.8.1",
    "gatsby": "^2.22.0",
    "gatsby-cli": "^2.12.43",
    "gatsby-image": "^2.4.3",
    "gatsby-plugin-i18n": "^1.0.1",
    "gatsby-plugin-manifest": "^2.4.2",
    "gatsby-plugin-offline": "^3.2.1",
    "gatsby-plugin-prefetch-google-fonts": "^1.4.3",
    "gatsby-plugin-react-helmet": "^3.3.1",
    "gatsby-plugin-sharp": "^2.6.2",
    "gatsby-remark-custom-blocks": "^2.3.1",
    "gatsby-source-filesystem": "^2.3.1",
    "gatsby-transformer-remark": "^2.8.7",
    "gatsby-transformer-sharp": "^2.5.2",
    "patch-package": "^6.2.2",
    "postinstall-postinstall": "^2.1.0",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.0.0",
    "sockjs-client": "^1.4.0",
    "start-server-and-test": "^1.11.0",
    "ts-node": "^8.10.1",
    "tsutils": "^3.17.1",
    "typescript": "^3.9.2",
    "utf-8-validate": "^5.0.2"
  },
  "devDependencies": {
    "prettier": "2.0.5"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "postinstall": "patch-package",
    "cy:open": "cypress open",
    "cy:run": "node ./node_modules/.bin/cypress run",
    "test:e2e": "start-server-and-test develop http://localhost:8000 cy:open",
    "test:e2e:ci": "start-server-and-test http://localhost:8000 cy:run",
    "test:e2e:local": "start-server-and-test develop http://localhost:8000 cy:run"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
